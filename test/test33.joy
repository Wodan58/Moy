(*
    Test character and string constants.
*)

(*
    This should be flagged as not allowed.
*)
'\000 .

(*
    This is also not allowed.
*)
"test\000ing\n" .

(*
    And this is not allowed.
    The bytecode compiler needs ^A as end-of-constant marker.
*)
"test\001ing\n" .
